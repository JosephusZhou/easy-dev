{
  "name": "easy-dev",
  "version": "0.1.2",
  "description": "A dev tool which can simplify operation for Android developer~",
  "main": "src/index.js",
  "scripts": {
    "start": "electron src/index.js",
    "build-osx": "electron-builder build --mac",
    "build-win": "electron-builder build --windows"
  },
  "build": {
    "productName": "EasyDev",
    "asar": true,
    "appId": "com.josephuszhou.easydev",
    "directories": {
      "output": "dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JosephusZhou/easy-dev"
  },
  "keywords": [
    "Android",
    "res",
    "tool"
  ],
  "author": "JosephusZhou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosephusZhou/easy-dev/issues"
  },
  "homepage": "https://github.com/JosephusZhou/easy-dev",
  "dependencies": {
    "jsqr": "^1.3.1",
    "nconf": "^0.10.0"
  },
  "devDependencies": {
    "asar": "^2.1.0",
    "electron": "^9.3.1",
    "electron-builder": "^22.6.1"
  }
}
